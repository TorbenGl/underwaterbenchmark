# Environment variables for docker-compose
# Copy this file to .env and fill in the values

# Required: HuggingFace token for DINOv3 model access
HF_TOKEN=your_huggingface_token_here

# Optional: Weights & Biases API key for experiment tracking
WANDB_API_KEY=your_wandb_key_here

# Data path on host machine
DATA_PATH=/path/to/your/data

# Logs path on host machine (will be created if doesn't exist)
LOGS_PATH=./logs

# HuggingFace cache directory (shared across containers)
HF_HOME=~/.cache/huggingface
